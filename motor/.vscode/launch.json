{
  // 使用 IntelliSense 了解相关属性。 
  // 悬停以查看现有属性的描述。
  // 欲了解更多信息，请访问: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
      
      {
          "name": "Cortex Debug",
          "cwd": "${workspaceRoot}",
          "executable": "${workspaceRoot}/build/${workspaceFolderBasename}.elf",
          "request":"launch",
          "type":"cortex-debug",
          "device":"STM32F103C6",        //使用J-link GDB Server时必须有；其他GBD Server时可选（有可能帮助自动选择SVD文件）。支持的设备见 https://www.segger.com/downloads/supported-devices.php
          "svdFile": "./STM32F103xx.svd",  //svd文件，有这个文件才能查看寄存器的值，每个单片机都不同。可以在以下地址找到 https://github.com/posborne/cmsis-svd
          "servertype": "jlink",         //使用的GDB Server
          "configFiles": [                  
            //  "${workspaceRoot}/openocd.cfg"
          ],
          "preLaunchTask": "build"
      }
  ]
}